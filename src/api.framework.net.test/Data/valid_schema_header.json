          "type": "Bit"
        },
        {
          "source": "client",
          "mapId": "14",
          "name": "IncludeVacation",
          "type": "Bit"
        },
        {
          "source": "client",
          "mapId": "15",
          "name": "RollingWeekend",
          "type": "Bit"
        },
        {
          "source": "client",
